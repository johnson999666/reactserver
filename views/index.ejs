            $(".header").addClass("sticky");
          } else {
            $(".header").removeClass("sticky");
          }
        });
  
        $('a[href^="#"]').click(function () {
          var target = $(this.hash);
          if (target.length) {
            $("html, body").animate(
              {
                scrollTop: target.offset().top,
              },
              1000
            );
            return false;
          }
        });
      });
