<script>
  const form = document.querySelector("#weather-form");
  const input = document.querySelector("#city");
  const suggestionList = document.querySelector("#suggestion-list");
  const weatherInfo = document.querySelector(".title2 p");
  
  const apiKey = "c740b141159dc93d62c32a163b009d37";
  
  form.addEventListener("submit", e => {
    e.preventDefault();
  
    const city = input.value.trim();
  
    // Clear the suggestion list and weather info
    suggestionList.innerHTML = "";
    weatherInfo.innerHTML = "";
  
    // Make a GET request to the weather API
    fetch(`https://api.openweathermap.org/data/2.5/weather?q=${city}&appid=${apiKey}&units=metric`)
      .then(response => {
        if (!response.ok) {
          throw new Error("City not found");
        }
        return response.json();
      })
      .then(data => {
        // Extract the relevant data from the response
        const temperature = Math.round(data.main.temp);
        const description = data.weather[0].description;
        const cityName = data.name;
  
        // Display the weather information to the user
        weatherInfo.innerHTML = `The current temperature in ${cityName} is ${temperature} degrees Celsius, with ${description}.`;
  
        // Clear the input field
        input.value = "";
      })
      .catch(error => {
    console.log("An error occurred: ", error);
    const errorElement = document.createElement("p");
    errorElement.textContent = `No results found for ${city}.`;
    weatherInfo.appendChild(errorElement);
  });
  
  });
  
