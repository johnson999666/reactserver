      <div class="change-color"></div>
      <script>
        const changeColorDiv = document.querySelector('.change-color');
        let isPastChangeColorDiv = false;
  
        window.addEventListener('scroll', function() {
          const body = document.querySelector('body');
          const scrollY = window.scrollY;
  
          // Check if user has scrolled past the changeColorDiv
          if (!isPastChangeColorDiv && scrollY > changeColorDiv.offsetTop + changeColorDiv.offsetHeight) {
            isPastChangeColorDiv = true;
            body.style.backgroundColor = 'rgb(124, 181, 181)';
          }
  
          // Check if user has scrolled back up past the changeColorDiv
          if (isPastChangeColorDiv && scrollY <= changeColorDiv.offsetTop) {
            isPastChangeColorDiv = false;
            body.style.backgroundColor = '#fff';
          }
        });
      </script>


